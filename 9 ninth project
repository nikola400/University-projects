// UPEx9.cpp : Defines the entry point for the console application.
// compares two numbers 

#include "stdafx.h"
#include <iostream>

using namespace std;

int main()
{
	int a = 0;
	int b = 0;
	cout << "Enter your first number a: ";
	cin >> a;
	cout << endl << "Enter your second number b: ";
	cin >> b;
	(a > b) ? (cout << endl << "a>b" << endl) : (cout << endl << "a<b" << endl);
    return 0;
}
