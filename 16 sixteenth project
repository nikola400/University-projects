// UPEx16.cpp : Defines the entry point for the console application.
//exercise of cycles 

#include "stdafx.h"
#include <iostream>

using namespace std;

int main()
{
	int n = 0;//redove
	int m = 0;//koloni
	cout << "Enter n: ";
	cin >> n;
	cout << "Enter m: ";
	cin >> m;

	for (int i = 0; i <= n-1; i++)
	{
		for (int j = 1; j <= m; j++)
		{
			cout << j+i*m << " ";
		}
		cout << endl;
	}
    return 0;
}
